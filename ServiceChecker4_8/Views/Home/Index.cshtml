@using ServiceChecker4_8.Models
@model IEnumerable<excModel>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Users</title>
</head>
<body>
    @using (Html.BeginForm("Index", "Home", FormMethod.Post))
    {
        <input type="submit" value="Tablo" />
    }
    @if (Model.Count() > 0)
    {
        <hr />
        <table cellpadding="0" cellspacing="0" border="1">
            <tr>
                <th>No</th>
                <th>Tc</th>
                <th>Ad Soyad</th>
                <th>Gün</th>
                <th>Ay</th>
                <th>Yıl</th>

            </tr>
            @foreach (excModel item in Model)
            {
        <tr>
            <td>@item.No</td>
            <td>@item.tcNo</td>
            <td>@item.AdiSoyadi</td>
            <td>@item.dogumAy</td>
            <td>@item.dogumAy</td>
            <td>@item.dogumYil</td>
        </tr>
            }
        </table>
    }
</body>
</html>